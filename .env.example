# .env - loaded in all environments
# .env.production - loaded only in production environments
# .env.development - loaded only in development environments

VITE_REMOVE_GENERATED_CITE=FALSE

# Backend Configuration
# Open source module (core services: auth, resources, conversations, etc.)
VITE_API_PATH=http://127.0.0.1:8000

# Commercial module (commercial services: pay, orders, products, feedback)
VITE_API_PATH_PRO=http://127.0.0.1:8001

# WebSocket Configuration
# Set to 'true' to use WebSocket instead of Server-Sent Events (SSE)
# Default: true (uses WebSocket)
VITE_USE_WEBSOCKET=true
